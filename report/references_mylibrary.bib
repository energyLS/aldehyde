@article{Emelianova,
  title = {Welfare Redistribution through Different Flexibility Deployment Strategies - {{Who}} Pays?},
  author = {Emelianova, Polina and Namockel, Nils},
  abstract = {Achieving the goal of climate neutrality calls for a decarbonization of end-use sectors. Hereby, the primary strategy is electrification of end-use applications for heating and transportation. In particular Germany has defined specific technology targets for the year 2030. The increased penetration of new technologies such as electric vehicles and heat pumps, has far-reaching implications for the energy system, notably influencing the formation of electricity prices. Moreover, the distinct behavioral constraints associated with various flexibility options disrupt the existing equilibrium between consumer and producer rents. This paper aims to address the following research questions: What is the impact of different flexibility deployment strategies on producer and consumer rents? To what extend do households have to be compensated to provide systemoriented flexibility? To address these questions, we further enhance the existing electricity market model DIMENSION. First, we develop a high-resolution dispatch model for Europe. Second, we increase the granularity in end-use sectors to account for flexible use of electric vehicles, including Vehicle-to-Grid/Building (V2X), heat pumps and heat storage. Doing so, electricity costs can be assigned to the individual actors, allowing an analysis of technology-specific welfare effects. We find that additional deployment of flexibility lowers the total system cost. Furthermore, we show that different flexibility deployment strategies impact technology-specific consumer and producer rents. For example, while the end-user procurement costs can be lowered by utilizing flexible charging, the costs do not decrease further by providing V2X services. As increase in consumer rents might not be sufficient to offset investment costs required to deploy flexibility, financial incentive schemes might be needed to unlock flexibility potential.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\J6YF9LR3\Emelianova and Namockel - Welfare redistribution through different flexibili.pdf}
}

