@inproceedings{Fioriti2023,
  title = {Country-{{Wise Open Energy Planning}} in {{High-Resolution}} with {{PyPSA-Earth}}},
  booktitle = {2023 {{International Conference}} on {{Smart Energy Systems}} and {{Technologies}} ({{SEST}})},
  author = {Fioriti, Davide and Fedotova, Ekaterina and Parzen, Maximilian and Giubilato, Denise and Frysztacki, Martha Maria and {Abdel-Khalek}, Hazem and Schumm, Leon and James, Stuart Daniel and Poli, Davide},
