@article{Merceron,
  title = {Climate Neutrality and Carbon Cycle of {{French}} Energy System: A Focus on e-Bio-Fuels Integration and Resources Requirements},
  author = {Merceron, Louis and Boissonnet, Guillaume and Mar{\'e}chal, Fran{\c c}ois},
  abstract = {CO2 emission reduction of sectors such as aviation, maritime shipping, road haulage and chemicals production is challenging, but necessary. Although these sectors will most likely continue to rely on carbonaceous energy carriers\textemdash especially for long-range aviation and chemicals\textemdash they are expected to gradually shift away from fossil fuels. In order to do so the prominent option is to utilize alternative carbon sources\textemdash like biomass and CO2 originating from carbon capture\textemdash for the production of non-fossil carbonaceous vectors (biofuels and e-fuels/RFNBOs). However, the limited availability of biomass and the varying nature of other carbon sources, necessitate a comprehensive evaluation of trade-offs between potential carbon uses and existing sources. This study utilizes the linear programming cost optimization tool Energyscope TD, to analyze the holistic French energy system, encompassing transport, industry, electricity, and heat sectors, while ensuring net greenhouse gas neutrality. We introduce a novel method to model and quantify carbon flows within the system, enabling a comprehensive assessment of greenhouse gas neutrality. Additionally, we perform a sensitivity analysis on biomass use and availability and carbon sequestration availability. The findings underscore the critical role of efficient carbon and energy valorization from biomass, offering valuable insights into the future cost of Sustainable Aviation Fuel (SAF) production for air travel originating from France but also on chemicals cost. Furthermore, our study highlights the significance of fulfilling clean energy requirements and implementing carbon dioxide removal measures as crucial steps toward achieving climate neutrality.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\5MG69PLN\Merceron et al. - Climate neutrality and carbon cycle of French ener.pdf}
}

