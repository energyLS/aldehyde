@article{Viole,
  title = {Techno-Environmental Multi-Objective Optimization of Energy Systems for an off-Grid Telescope},
  author = {Viole, Isabelle and {Valenzuela-Venegas}, Guillermo and Sartori, Sabrina and Zeyringer, Marianne},
  abstract = {We integrate life cycle greenhouse gas emissions into our energy system model, which optimizes an off-grid system to supply a telescope's power demand in Chile with mostly solar power. Using multi-objective optimization, minimizing both costs and life cycle greenhouse gas emissions, we find the Pareto boundary for this case study. We compare this ex-ante setup to a prior ex-post life cycle assessment, and furthermore dive into sensitivities of our model regarding battery lifetimes. We find (i) low-hanging fruit in lowering emissions with manageable cost increases of \$0.02/MWh to decrease the carbon intensity by 1kg CO2e/MWh, (ii) ex-ante life cycle assessments' possibility to find less-GHG intensive power systems compared to ex-post conducted studies, (iii) a heavy sensitivity of the optimization model on assumed battery lifetimes. This study shows the importance of including life cycle CO2e emissions into the optimization objective of energy system models to see the environmental and economic trade-offs associated with off-grid RES-based energy systems.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\JMAMWLM2\Viole et al. - Techno-environmental multi-objective optimization .pdf}
}

