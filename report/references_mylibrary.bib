@article{Sultani,
  title = {Optimal De-Risking Strategies for Breakthrough Technologies: {{Risk}} Allocation in the Transformation of the Basic Materials Industry},
  author = {Sultani, Darius and Pahle, Michael and Jakob, Michael},
  abstract = {The technological solutions for the basic materials industry's transformation towards climate neutrality are known. Green technology deployment, however, has still not occurred at scale and remains below socially optimal levels. Analysing the means by which the regulator should align private investment conditions with social welfare, we formulate an analytical model to assess the optimal policy intervention when a carbon price is in place to internalise emissions externalities, but additional market failures are present. With our paper, we aim to a) structure the instrument design debate for the industry transformation, b) systematically bring risk considerations of the regulator into the picture and c) inform how design features of proposed instruments affect the risk-return profile of both regulator and producer. For each of the considered market failure scenarios, we consider an output subsidy as a strictly revenue-increasing intervention, as well as a carbon contract and a green lead market instrument as interventions with both a revenue-increasing and a risk-reducing policy component. In the base case with positive technology externalities from green production, we find that risk should not be transferred away from the producer and onto the public when the producer has the means to efficiently hedge on private future markets. Introducing incomplete future markets as a second market failure on top of the base scenario, we derive that the optimal policy intervention depends on the risk aversion of the regulator and the producer. Consequently, the social planner should aim to allocate carbon price or revenue risk to the actor that shows the lower risk aversion. Thirdly, we analyse the case of information asymmetries between the regulator and the producer. More specifically, we assume that the regulator does not observe the producer's hedging efficiency. While the initial findings from the base model hold, we show for this case that risk-transferring policy interventions can prevent information rents on risk components that are transferred to the public.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\UVY3PV7W\Sultani et al. - Optimal de-risking strategies for breakthrough tec.pdf}
}

