@article{Brandkamp,
  title = {Hedging Households against Extreme Electricity Prices},
  author = {Brandkamp, Simon},
  abstract = {This paper explores how to complement dynamic electricity tariffs with forward hedging to protect households from extremely high prices. Using a utility maximization model, I simulate the optimal share of electricity consumption that households should buy forward for a sample of 2,159 UK households. I investigate the impact of price elasticity of demand and risk aversion on optimal hedge shares. The key finding is that an increase in price elasticity either raises or lowers optimal hedge shares, depending on the sign of the correlation between electricity prices and households' weatherrelated desire to consume electricity. Simulations reveal an average optimal hedge share of 59\%, with higher shares for electric heating and electric vehicle owners and lower shares for solar PV and battery storage owners. Forward hedging significantly reduces monthly electricity bill volatility by 19\% for relatively price-inelastic households. However, welfare gains from optimal forward hedging are modest, translating to only a 0.3\% reduction in average electricity prices.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\TSFRNI86\Brandkamp - Hedging households against extreme electricity pri.pdf}
