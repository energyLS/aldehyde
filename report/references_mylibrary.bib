@article{Rahmad,
  title = {Spatial Heterogeneity in Deployment and Upscaling of Wind Power in {{Swedish}} Municipalities},
  author = {Rahmad, Yodefia},
  abstract = {Wind power often diffuses unevenly across a country, despite uniform policy instruments and electricity prices. Understanding where and how wind power has historically been deployed can help inform future development of wind power and consequently the feasibility of national targets. In this paper, we investigated deployment of onshore wind power in Swedish municipalities over the last 30 years. We identified factors that affect spatial heterogeneity in wind deployment using statistical analysis and examined the common features of municipalities with the highest levels of deployment. Our analysis found that onshore wind power in Sweden has increased both in unit capacity and number of units in a single wind park. Smaller wind power projects were deployed at lower rate compared to larger projects and have saturated since 2010. Municipalities with high share of agricultural land use and longer time since initial deployment have higher installed capacity of small-scale wind power projects, highlighting the importance of suitable land cover and accumulation of experience over the years. On the other hand, municipalities with a high share of prioritized area for wind power have deployed more large-scale wind power projects, indicating the relevance of siting recommendations from regulatory authorities for more recent deployment. Land constraints such as population density and nature reserves do not have significant effect on deployment of either scale of wind power projects. Additionally, we found that municipalities with highest installed capacity of large-scale projects consistently have a high amount of forested land and exhibit a range of acceptable wind speeds. We also observed that high levels of deployment are typically achieved by a few large projects without any precursors. Our research demonstrates the necessity to distinguish between different scales of wind power projects when analyzing historical deployment, due to the different mechanisms at play. This approach can serve as a useful framework for examining subnational deployment of wind power, particularly when upscaling occurs or when other changes are introduced to the technology.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\GBSQ7PAL\Rahmad - Spatial heterogeneity in deployment and upscaling .pdf}
}

