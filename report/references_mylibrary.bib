@article{Steenberghe,
  title = {Who Are the Citizens Participating in Renewable Energy Cooperatives? {{A}} Study Based on Both Stated Preferences and Trace Data},
  author = {Steenberghe, Melita Van and D'hulster, Aislinn and Weytjens, Johannes and Ovaere, Marten and Schoors, Koen},
  abstract = {The European Union has set a target of at least 45\% of renewable energy sources in its overall energy consumption by 2030. To accomplish this target, the EU emphasises the role of energy cooperatives. These cooperatives prioritise the production of renewable energy from citizen-owned sources, ensuring that energy sharing becomes a fundamental principle. This paper delves into citizens' participation in energy cooperatives, focusing on the profile of current members, as well as identifying which social groups are still underrepresented in energy cooperatives today. The analysis combines insights from two data sources. First, an online survey of around 1,500 members of two energy cooperatives in Flanders and around 1,500 non-cooperative members. Second, exclusive pseudonymized transaction data of residential consumers of a large Belgian bank, BNP Paribas Fortis (BNPPF). By examining socio-demographics such as gender, age, income, wealth, and the willingness and ability to invest, profiles of cooperative members and non-cooperative members are obtained. In addition, the analysis of socio-demographics in relation to investment patterns uncovers noteworthy trends and enables the grouping of individuals based on their characteristics. Furthermore, by assessing the willingness and ability to invest, valuable insights can be gained regarding the overall potential of these cooperatives. Identifying the profile types of cooperative members serves as a necessary first step in the realisation of the vision of the EU to provide renewable energy for all citizens. It allows policymakers to implement measures that extend access to energy cooperatives and their renewable energy production, particularly for lower-income groups who remain excluded from such front running initiatives. Given the opportunities in this field, research plays a crucial role in comprehending the socio-economic aspects of energy cooperatives and their investors, guiding policy decisions, and developing innovative solutions to overcome barriers.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\YLX7NWSN\Steenberghe et al. - Who are the citizens participating in renewable en.pdf}
}

