@article{Laveneziana2023,
  title = {Critical Review of Energy Planning Models for the Sustainable Development at Company Level},
  author = {Laveneziana, Lorenzo and Prussi, Matteo and Chiaramonti, David},
  year = {2023},
  month = sep,
  journal = {Energy Strategy Reviews},
  volume = {49},
  pages = {101136},
