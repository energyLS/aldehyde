@article{Gueret,
  title = {Power System Impacts of E-Carsharing in {{Germany}} in 2030},
  author = {Gu{\'e}ret, Adeline and Schill, Wolf-Peter and {Gaete-Morales}, Carlos},
  abstract = {As the stock of battery-electric vehicles (BEVs) is growing at increasing speed, new questions emerge when it comes to the sustainability of the electrification of the passenger road transport sector. As electrifying the whole vehicle fleet might bring adverse effects in terms of mineral resource use, a reduction of the overall car fleet size might be needed. Electric carsharing has the potential to speed up the electrification of the passenger car fleet while decreasing its size. Yet, it could also reduce the flexibility potential of BEVs in the power system. In this paper, we use an open-source power sector model (DIETERpy) and an open-source vehicle time series tool (emobpy) to study the impacts of electric carsharing on the power sector in Germany in 2030, assuming a future power system highly t dominated by renewable energy sources (RES). In order to overcome computational challenges related to including detailed BEV time series in a power system model, we apply sequence clustering techniques to derive representative car user statistics and identify users that are most likely to switch to carsharing in the mid-term. Our results show that switching to electric carsharing moderately increases costs in a flexibility-constrained power sector. This cost f effect is largely mitigated when considering more realistic settings taking into account other sources of flexibility. We conclude that electric carsharing does not hurt the power sector much but potentially brings along many other benefits.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\G6ANEEQZ\Guéret et al. - Power system impacts of e-carsharing in Germany in.pdf}
}

