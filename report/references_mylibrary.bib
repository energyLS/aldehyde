@article{Ruhnau2023a,
  title = {Flexible Green Hydrogen: {{The}} Effect of Relaxing Simultaneity Requirements on Project Design, Economics, and Power Sector Emissions},
  shorttitle = {Flexible Green Hydrogen},
  author = {Ruhnau, Oliver and Schiele, Johanna},
