@article{Hubner,
  title = {Bids on {{Combinatorial Electricity Auctions}}: {{Why Europe}} Is Not as Bad off as Thought.},
  author = {Hubner, Thomas},
  abstract = {Day-ahead electricity markets are organized as combinatorial auctions where producers and consumers can trade power for each hour of the next day simultaneously instead of hour by hour. In Europe, this is possible through bids on blocks of multiple rather than just individual hours. However, the many possible combinations of hours make the decision space exponentially large and the bidding decision complex. This inherent bidding complexity of combinatorial auctions poses a fundamental problem: an efficient allocation can only be achieved if each individual is able to truly reflect their preferences through their bids. In this paper, we show how agents can overcome this complexity by using price information. To this end, we exploit total unimodularity to present a linear program to determine an exclusive group of bids. Moreover, we link the quality of these bids to the available price information measured by the Wasserstein distance. We demonstrate the efficiency of this bidding approach on three cases studies of a thermal generator, a battery storage and a district heating utility selling and buying electricity on the German day-ahead market. Finally, we use these theoretical and empirical results to inform the policy debate around the design of bid formats on day-ahead markets.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\28685KTY\Hubner - Bids on Combinatorial Electricity Auctions Why Eu.pdf}
}

