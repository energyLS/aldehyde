@article{Boning,
  title = {The {{Effect}} of {{Future Financial Benefits}} on {{PV Adoption Patterns}}},
  author = {Boning, Justus and Bruninx, Kenneth and Ovaere, Marten and Delarue, Erik and Pepermans, Guido},
  abstract = {We assess the effect of different incentive schemes with future financial benefits on photovoltaic (PV) adoption patterns in the residential sector for the two biggest regions of Belgium - Flanders and Wallonia. Due to variation in benefit schemes across the regions and within regions across months, we can identify the effect of changes in future benefits on adoption. We combine and compare three distinct and widely used future benefit schemes for PV, namely output-based, capacity-based and electricity cost saving-based. We find that PV adoption in the residential sector is highly sensitive to overall net-benefits and that the different benefit schemes have varying degrees of effectiveness. In particular, capacity- and output-based benefit schemes, which directly compensate households with a fixed price per produced electricity or per installed capacity annually (direct benefits), respectively, are above 60\% more effective than the electricity cost saving-based scheme, which depends on future electricity prices (indirect benefits). Furthermore, depending on the specific benefit scheme, the average newly installed capacity can either increase or decrease.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\IGRNBN76\Boning et al. - The Effect of Future Financial Benefits on PV Adop.pdf}
}

@misc{Bottcher2023,
  title = {Representation of {{Distribution Grid Expansion Costs}} in {{Power System Planning}}},
  author = {B{\"o}ttcher, Luis and Fr{\"o}hlich, Christian and Kortmann, Steffen and Braun, Simon and Saat, Julian and Ulbig, Andreas},
  year = {2023},
  month = oct,
  number = {arXiv:2310.11853},
