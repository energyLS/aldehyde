@article{Johannsen,
  title = {Draft {{Version}} for {{YEES Conference}} 5th-6th {{October}} 2023 {{How}} Does Cash Windfall Impact Firm Investment? {{Evidence}} from the {{US}} Power Market},
  author = {Johannsen, Alida and Schwenen, Sebastian},
  abstract = {Recently, increasing fossil fuel prices have led to substantial windfall profits for energy companies. Thereby, little is known how firms utilize cash windfall in the energy market. This paper examines the impact of cash windfall on investment behavior in power markets. In practice, it is challenging to empirically distinguish between cash windfall and profit from firm performance. We exploit extreme temperature as a novel instrument for two-stage instrumental variable regression to isolate cash windfall from firm profit and to show how power plants utilize cash windfall. We uses rich data on investment in power generation assets from the U.S. power market between 2008 and 2021. We contribute by exploring (i) how firms change their investment behavior in the presence of cash windfall and (ii) if this investment is directed towards improved environmental performance. We find that higher temperature increases plant revenue, which leads to a significant increase in total capital expenditures and environmental performance.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\F87AIRVA\Johannsen and Schwenen - Draft Version for YEES Conference 5th-6th October .pdf}
}

@article{Kapica2024,
  title = {The Potential Impact of Climate Change on {{European}} Renewable Energy Droughts},
  author = {Kapica, Jacek and Jurasz, Jakub and Canales, Fausto A. and Bloomfield, Hannah and Guezgouz, Mohammed and De Felice, Matteo and Zbigniew, Kobus},
  year = {2024},
  month = jan,
  journal = {Renewable and Sustainable Energy Reviews},
  volume = {189},
  pages = {114011},
  issn = {1364-0321},
  doi = {10.1016/j.rser.2023.114011},
  urldate = {2023-11-06},
  abstract = {The daily, seasonal, and interannual variability of solar and wind resources is well-documented, based on evidence from multi-decadal meteorological time series. However, with the growing share of non-dispatchable renewable-based power sources (e.g., wind and solar power), the stable operation of the power system could be undermined by prolonged periods of low availability of these resources. Consequently, this may result in extremely high prices in the energy market or even a power system blackout. This study analyzes the performance of solar, wind, and solar-wind hybrid systems in Europe based on eight regional climate models, considering two possible climate change projections. The resource availability has been evaluated based on the energy drought concept. The total duration of droughts is calculated using daily capacity factors covering the years 1970\textendash 2020 (reference period) and 2048\textendash 2098 (future period), considering sub-national regions across the whole of Europe. In general, the chosen climate models show a more significant agreement in the occurrence of energy droughts for northern and southern Europe compared to its central part. Assessing the potential for renewable energy droughts is critical to maintaining secure and reliable power system operation in both the present and future climate.},
  keywords = {Europe,Extreme events,Future scenarios,Hybridization,Resource droughts,Spatial representation}
}

@article{Kettani2020,
