  author = {Schumm, Leon and {Abdel-Khalek}, Hazem and Brown, Tom and Ueckerdt, Falko and Sterner, Michael and Parzen, Maximilian and Fioriti, Davide},
  year = {2023},
  journal = {[Under Review]},
  abstract = {Several countries are emerging as future potential exporters of green hydrogen, while simultaneously pursuing ambitious energy transitions to reduce greenhouse gas emissions. Morocco serves as a blueprint for countries aiming to design their energy systems along these two dimensions, risking technology lock-in, unabated local energy-related greenhouse gas emissions, rising local energy prices, land use implications and neo-colonialism. Extending the current state of research, we conduct an integrated analysis of synergies and con icts between these two objectives, taking into account the potential impact of hydrogen exports on economic bene ts for both local populations and hydrogen exporters. We present a fully sector-coupled capacity expansion and dispatch model of Morocco across 53 regions, including integrated gas pipeline and electricity network planning based on PyPSA-Earth. e applied sector-coupled model simulates and optimises the Moroccan energy system in more than 100 scenarios at a 3-hourly temporal resolution, sweeping through di erent hydrogen export volumes and climate targets. Results show that hydrogen exports could provide signi cant economic bene ts for both hydrogen exporters and the local population, but also create potential con icts and rising electricity and hydrogen prices. In addition, potential risks of neo-colonialism highlight the need for an integrated assessment of hydrogen exports and local decarbonisation. Integrated policies unlock synergies and minimise potential con icts between hydrogen exports and Morocco's energy transition, while ensuring social and environmental sustainability.},
  langid = {english},
  file = {C:\Users\scl38887\Zotero\storage\PNILC9CG\Schumm et al. - Synergies and Con icts of Hydrogen Export and Nati.pdf}
}

